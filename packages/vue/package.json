  "devDependencies": {
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "bili": "^5.0.5",
    "eslint-plugin-vue": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "vue": "^3.0.4"
  }
